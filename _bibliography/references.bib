  author = {Castellano, Giovanna and Esposito, Andrea and Mirizio, Marco and Montanaro, Graziano and Vessio, Gennaro},
  booktitle = {Proceedings of the 2021 IEEE Symposium Series on Computational Intelligence},
  date-added = {2022-10-16 09:45:32 +0200},
