@inproceedings{Greco2023A-Human-Centered,
	author = {Greco, Francesco and Desolda, Giuseppe and Esposito, Andrea},
	booktitle = {Proceedings of the ACM CHI 2023 Workshop on Human-Centered Explainable AI},
	date-added = {2023-04-17 09:08:57 +0200},
	date-modified = {2023-05-06 18:23:29 +0200},
	keywords = {Phishing, XAI, Human-Centered Design},
	series = {HCXAI 2023},
	title = {A Human-Centered XAI System for Phishing Detection},
	url = {https://www.dropbox.com/s/kiutrmgm8kcjoc4/HCXAI2023_paper_3020.pdf?dl=0},
	venue = {Hamburg, Germany},
	year = {2023},
	abstract = {AI models are widely used in cybersecurity to identify digital content that could be malicious. This is also the case with phishing emails: when a defense system detects a suspicious message, users are typically presented with warning dialogs that inform them of the risks. However, warnings are often designed without adequate consideration of the end user, leading to incorrect decisions that lower the benefit of the AI models. To overcome the limitations identified in the literature, this paper presents an XAI phishing detection system built following a human-centered design approach. It classifies phishing emails and generates polymorphic warning dialogs that explain to the user why the email might be a scam, with the ultimate goal of supporting a more informed decision on whether or not to open suspicious content.},
	bdsk-file-1 = {},
	bdsk-url-1 = {https://www.dropbox.com/s/kiutrmgm8kcjoc4/HCXAI2023_paper_3020.pdf?dl=0}}

